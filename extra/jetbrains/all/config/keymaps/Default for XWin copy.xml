    <keyboard-shortcut first-keystroke="shift ctrl v" />
  </action>
  <action id="CopyPaths" />
  <action id="DelegateMethods">
    <keyboard-shortcut first-keystroke="alt c" second-keystroke="d" />
  </action>
  <action id="Editor Copy">
    <keyboard-shortcut first-keystroke="shift ctrl c" />
  </action>
  <action id="Editor Paste">
    <keyboard-shortcut first-keystroke="shift ctrl v" />
  </action>
  <action id="EditorBackSpace">
    <keyboard-shortcut first-keystroke="back_space" />
    <keyboard-shortcut first-keystroke="shift back_space" />
